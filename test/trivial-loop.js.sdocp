sdocp('test/trivial-loop.js.sdoc', 'Trivial loop on ECX test.\nImplements this logic:\n\n| xorl %ebx, %ebx\n  movl $1, %eax\n  movl $100, %ecx\n  .top:\n    addl %ecx, %ebx\n    dec %ecx\n    test %ecx, %ecx\n    ja .top\n  int $0x80\n\ncaterwaul.clone(\'std seq llasm.numerics llasm.elf llasm.offsets\')(function (require) {\n  var bytes = caterwaul.llasm.elf32.trivial_code(qas[x31 /b11_011011, xb001_b164\n                                                    |+top, x01 /b11_001011\n                                                         | xff /b11_001001\n                                                         | x85 /b11_001001\n                                                         | x77 /top[-1].r8\n                                                    |xcb80]);\n  require(\'fs\') /se.fs[\n    fs.writeFile(\'test/return-550\', new Buffer(bytes.length) /se.bs[seq[bytes *![bs[_i] = _]]], fn_[fs.chmodSync(\'test/return-550\', 0700)])];\n})(require);\n');