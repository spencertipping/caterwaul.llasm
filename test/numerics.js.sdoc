Numerical macro tests.

caterwaul.clone('std seq llasm.numerics')(function () {
  var eight = b00001000;
  eight.length === 1 || null['b00001000.length should not be #{eight.length}'];
  eight[0]     === 8 || null['b00001000[0] should not be #{eight[0]}'];

  x40.length === 1 || null['x40.length should not be #{x40.length}'];
  x40[0] === 64 || null['x40[0] should not be #{x40[0]}'];

  xabcdef.length === 3 || null['xabcdef.length should not be #{xabcdef.length}'];
  xabcdef[0] === 0xab || null['xabcdef[0] should be #{0xab}, not #{xabcdef[0]}'];
  xabcdef[1] === 0xcd || null['xabcdef[1] should be #{0xcd}, not #{xabcdef[1]}'];
  xabcdef[2] === 0xef || null['xabcdef[2] should be #{0xef}, not #{xabcdef[2]}'];

  x4050.length === 2 || null['x4050.length should not be #{x4050.length}'];
  x4050[0] === 64 || null['x4050[0] should not be #{x4050[0]}'];
  x4050[1] === 80 || null['x4050[1] should not be #{x4050[1]}'];

  x40_5_0_[1] === x4050[1] || null['x40_5_0_ should not be #{x40_5_0_.toString()}'];
})();
